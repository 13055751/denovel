{
  "name": "denovel",
  "version": "0.5.0",
  "description": "A deno project aiming to make downloading and processing novel as easy as pie",
  "main": "main.ts",
  "scripts": {
    "build": "deno compile --unstable-raw-imports --unstable-kv --allow-all --include=data.d.ts --include templates --include static  --output=app.exe exe.ts",
    "serve": "deno run --unstable-raw-imports --unstable-kv --allow-all server.ts"
  },
  "keywords": [
    "novel",
    "epub",
    "txt"
  ],
  "author": "imzlh",
  "license": "MIT",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@types/ejs": "^3.1.5",
    "@types/hast": "^3.0.4",
    "ejs": "^3.1.10",
    "entities": "^6.0.1",
    "image-size": "^2.0.2",
    "mime": "^4.0.7",
    "rehype": "^13.0.2",
    "rehype-parse": "^9.0.1",
    "rehype-stringify": "^10.0.1",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  }
}
